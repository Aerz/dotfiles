echo "########################################"
echo "Installing Mac App Store apps"
echo "########################################"

mas_apps=(
  935235287   # Encrypto
  497799835   # XCode
  775737590   # iA Writer
  407963104   # Pixelmator
  568494494   # Pocket
  880001334   # Reeder 3
  1176895641  # Spark Email
  1384080005  # Tweetbot 3
  410628904   # Wunderlist
)

for app in ${mas_apps[@]};
do
  mas install "$app"
done
