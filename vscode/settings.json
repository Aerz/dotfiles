{
  "editor.fontFamily": "FiraCode-Retina",
  "editor.lineHeight": 17,
  "editor.fontLigatures": true,
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "workbench.fontAliasing": "auto",
  "debug.toolBarLocation": "docked",
  "editor.minimap.enabled": false,
  "explorer.openEditors.visible": 0,
  "editor.quickSuggestionsDelay": 0,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.insertFinalNewline": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "eslint.alwaysShowStatus": true,
  "prettier.requireConfig": true,
  "git.autofetch": true,
  "git.detectSubmodules": false,
  "gitlens.views.compare.files.layout": "auto",
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.keymap": "alternate",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": false
  },
  "files.associations": {
    "zshrc": "shellscript",
    "Vagrantfile": "ruby",
    "*.service": "properties"
  },
  "breadcrumbs.enabled": true,
  "terminal.integrated.fontSize": 12,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "workbench.iconTheme": "file-icons",
  "workbench.colorCustomizations": {
    "terminal.background":"#1f1f1f",
    "terminal.foreground":"#e3e3e3",
    "terminalCursor.background":"#e2e2e2",
    "terminalCursor.foreground":"#cbcbcb",
    "terminal.ansiBlack":"#2d2d2d",
    "terminal.ansiBrightBlack":"#505050",
    "terminal.ansiBlue":"#6699cc",
    "terminal.ansiBrightBlue":"#78b7fe",
    "terminal.ansiCyan":"#5fa7a7",
    "terminal.ansiBrightCyan":"#71c7c7",
    "terminal.ansiGreen":"#80b080",
    "terminal.ansiBrightGreen":"#99cc99",
    "terminal.ansiMagenta":"#9078ba",
    "terminal.ansiBrightMagenta":"#b392f0",
    "terminal.ansiRed":"#c1595b",
    "terminal.ansiBrightRed":"#f2777a",
    "terminal.ansiWhite":"#cecece",
    "terminal.ansiBrightWhite":"#eaeaea",
    "terminal.ansiYellow":"#d9ad56",
    "terminal.ansiBrightYellow":"#ffcc66"
  },
  "terminal.external.osxExec": "iTerm.app",
  "window.nativeTabs": true,
  "editor.renderWhitespace": "all",
  "workbench.colorTheme": "Min Dark",
  "webhint.enableTelemetry": "disabled"
}
